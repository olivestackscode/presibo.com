{
  "name": "presibo.com",
  "version": "1.0.0",
  "description": "AI-powered healthcare platform for Nigerian healthcare access",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "build": "node scripts/build.js",
    "build:production": "NODE_ENV=production node scripts/build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint server/ scripts/",
    "lint:fix": "eslint server/ scripts/ --fix",
    "deploy": "node scripts/deploy.js",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker-compose up -d",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "validate-env": "node scripts/validate-env.js",
    "prestart": "npm run validate-env",
    "postbuild": "echo 'Build completed. Deploy the dist/ directory to your hosting provider.'"
  },
  "keywords": [
    "healthcare",
    "ai",
    "telemedicine",
    "nigeria",
    "anthropic",
    "medical"
  ],
  "author": "Presibo Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olivestackscode/presibo.com.git"
  },
  "bugs": {
    "url": "https://github.com/olivestackscode/presibo.com/issues"
  },
  "homepage": "https://presibo.com"
}